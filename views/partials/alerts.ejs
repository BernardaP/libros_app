<% if (alerts.error) { %>
  <% alerts.error.forEach(msg => { %>
    <div class="alert alert-error"><span class="closebtn">&times;</span><%= msg %></div>
  <% }); %>
<% } %>
<% if (alerts.success) { %>
  <% alerts.success.forEach(msg => { %>
    <div class="alert alert-success"><span class="closebtn">&times;</span><%= msg %></div> 
  <% }); %>
<% } %>